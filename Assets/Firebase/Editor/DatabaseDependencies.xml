<dependencies>
	<iosPods>
		<iosPod name="Firebase/Database" version="10.25.0" minTargetSdk="8.0">
		</iosPod>
	</iosPods>
	<androidPackages>
		<androidPackage spec="com.google.firebase:firebase-database:21.0.0">
		</androidPackage>
		<!-- Firebase Analytics bağımlılığı, Firebase ile ilgili diğer işlemler için gerekli -->
		<androidPackage spec="com.google.firebase:firebase-analytics:22.0.0">
		</androidPackage>
		<!-- Firebase Database Unity bağımlılığı -->
		<androidPackage spec="com.google.firebase:firebase-database-unity:12.0.0">
			<repositories>
				<repository>Assets/Firebase/m2repository</repository>
			</repositories>
		</androidPackage>
	</androidPackages>
</dependencies>
